<script>
    // Função para encolher a primeira imagem
    function shrinkFirstImage() {
        const firstImage = document.getElementById('first-image');
        firstImage.classList.remove('w-80'); // Remove a largura expandida
        firstImage.classList.add('w-20'); // Define a largura encolhida
    }

    // Função para expandir a primeira imagem novamente
    function expandFirstImage() {
        const firstImage = document.getElementById('first-image');
        if (!firstImage.classList.contains('hover:w-80')) {
            firstImage.classList.remove('w-20'); // Remove a largura encolhida
            firstImage.classList.add('w-80'); // Define a largura expandida
        }
    }

    // Quando a página carrega, expande a primeira imagem
    document.addEventListener('DOMContentLoaded', function () {
        const firstImage = document.getElementById('first-image');
        firstImage.classList.add('w-80'); // Adiciona a classe para a largura expandida
    });
</script>

    
<script>
