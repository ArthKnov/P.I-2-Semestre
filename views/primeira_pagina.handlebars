        author: "Emily Clark",
        company: "Amazon",
      },
      {
        text: "I was thoroughly impressed with the professionalism and skill at Mira Salon. My nails have never looked better!",
        author: "Sarah Brown",
        company: "Apple",
      },
      {
        text: "Amazing atmosphere and incredible service. Mira Salon has won me over for life!",
        author: "Robert Lee",
        company: "Facebook",
      },
    ];

    let currentIndex = 0;

    function showTestimonial(index) {
      const testimonialContainer = document.getElementById("testimonials");
      testimonialContainer.innerHTML = `
        <div class="text-lg text-gray-800 mb-4">★★★★★</div>
        <p class="text-pink-500 text-[25px] font-light italic mb-4 mx-auto w-[50%]">
          "${testimonials[index].text}"
        </p>
        <div class="text-4x1 text-gray-800 mt-4">- ${testimonials[index].author}, <span class="font-bold">${testimonials[index].company}</span></div>
      `;

      // Atualiza as bolinhas de navegação
      document.querySelectorAll(".dot").forEach((dot, idx) => {
        dot.classList.toggle("bg-gray-800", idx === index);
        dot.classList.toggle("bg-gray-400", idx !== index);
      });
    }

    function nextTestimonial() {
      currentIndex = (currentIndex + 1) % testimonials.length;
      showTestimonial(currentIndex);
    }

    function prevTestimonial() {
      currentIndex = (currentIndex - 1 + testimonials.length) % testimonials.length;
      showTestimonial(currentIndex);
    }

    function currentTestimonial(index) {
      currentIndex = index;
      showTestimonial(currentIndex);
    }

    // Exibe o primeiro depoimento ao carregar a página
    showTestimonial(currentIndex);

    // Configura a rolagem automática a cada 5 segundos
    setInterval(nextTestimonial, 5000);
  </script>




